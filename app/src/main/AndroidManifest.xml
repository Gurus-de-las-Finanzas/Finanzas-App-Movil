<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Finanzas"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".vantir.controller.activities.VanTirResultsActivity"
            android:exported="false" />
        <activity
            android:name=".vantir.controller.activities.NetFlowActivity"
            android:exported="false" />
        <activity
            android:name=".vantir.controller.activities.VanTirFormActivity"
            android:exported="false" />
        <activity
            android:name=".payments.controller.activities.PaymentPlanActivity"
            android:exported="false" />
        <activity
            android:name=".payments.controller.activities.PaymentFormActivity"
            android:exported="false" />
        <activity
            android:name=".clients.controller.activities.AddClientActivity"
            android:exported="false" />
        <activity
            android:name=".clients.controller.activities.ClientsActivity"
            android:exported="false" />
        <activity
            android:name=".home.controller.activities.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".security.controller.activities.RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".security.controller.activities.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>